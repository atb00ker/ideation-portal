{
  "name": "ideation-portal",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Ajay Tripathi <ajay39in@gmail.com> (https://atb00ker.github.io)",
  "homepage": "https://github.com/atb00ker/ideation-portal#readme",
  "description": "Ideation portal for companies to collect ideas from bottom-up.",
  "scripts": {
    "docker": "docker-compose up -d",
    "metadata": "hasura metadata apply --log-level=DEBUG",
    "metadata-reload": "hasura metadata reload --log-level=DEBUG",
    "migrate": "hasura migrate apply --log-level=DEBUG --all-databases",
    "hasura": "hasura console --skip-update-check --no-browser --log-level=DEBUG",
    "actions": "env-cmd fresh -c gofresh.config",
    "docker-test": "docker-compose --env-file .env.test -f docker-compose.test.yml up -d",
    "metadata-test": "npm run metadata -- --envfile=.env.test",
    "migrate-test": "npm run migrate -- --envfile=.env.test && npm run metadata-reload -- --envfile=.env.test",
    "actions-test": "env-cmd -f .env.test npm run test",
    "integration-test": "env-cmd -f .env.test go test ./... -tags=integration_tests",
    "unit-test": "env-cmd -f .env.test go test ./... -tags=unit_tests",
    "coverage": "go test ./... -cover -tags=all_tests",
    "test": "env-cmd -f .env.test npm run coverage",
    "react-start": "webpack serve --open --mode development",
    "react-build": "webpack --mode production",
    "react-test": "jest --runInBand --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atb00ker/ideation-portal.git"
  },
  "keywords": [
    "ideation-portal"
  ],
  "bugs": {
    "url": "https://github.com/atb00ker/ideation-portal/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.1",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "bootstrap": "^5.0.2",
    "css-loader": "^6.2.0",
    "dotenv-webpack": "^7.0.3",
    "env-cmd": "^10.1.0",
    "file-loader": "^6.2.0",
    "hasura-cli": "^2.0.3",
    "html-webpack-plugin": "^5.3.2",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.0-beta.4",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "^17.0.2",
    "react-use-websocket": "^2.7.1",
    "sass": "^1.36.0",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "typescript": "^4.3.5",
    "webpack": "^5.47.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.6.0"
  }
}
